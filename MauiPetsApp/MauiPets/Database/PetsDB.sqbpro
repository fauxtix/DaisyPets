            Pet.Chip, Pet.Chipado, Pet.DataChip, Pet.NumeroChip,  Pet.Esterilizado, Pet.Padrinho,

            R.Descricao AS [RacaAnimal], Pet.Medicacao AS [MedicacaoAnimal],

            E.Descricao AS [EspecieAnimal], T.Descricao AS [TamanhoAnimal],

            Temp.Descricao AS [TemperamentoAnimal], Pet.Genero,

            S.Descricao AS [SituacaoAnimal], Pet.DataNascimento

            FROM Pet INNER JOIN Raca R ON Pet.IdRaca = R.Id

            INNER JOIN Especie E ON Pet.IdEspecie = E.Id

            INNER JOIN Tamanho T ON Pet.IdTamanho = T.Id

            INNER JOIN Situacao S ON Pet.IdSituacao = S.Id

            INNER JOIN Temperamento Temp ON Pet.IdTemperamento = Temp.Id

</sql><current_tab id="0"/></tab_sql></sqlb_project>
