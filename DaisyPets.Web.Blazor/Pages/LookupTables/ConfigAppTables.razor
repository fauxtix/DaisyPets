@page "/admin/configapptables"
@using Microsoft.Extensions.Localization;
@inject IStringLocalizer<App> L

<PageTitleComponent PageTitle="@L["lblManutencao"]" WithBadge=true PageSubTitle="@L["menu-admin-lookup-tables"]" />
<DisplayApplicationTables />

@code {
    private string SimpleActiveItem = "./admin/lookuptables/displayapplicationtables";
}

<style>
    @@font-face {
        font-family: 'e-bicons';
        src: url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype');
        font-weight: normal;
        font-style: normal;
    }

    .e-bicons {
        font-family: 'e-bicons' !important;
        font-size: 14px;
    }

    .e-breadcrumb .e-breadcrumb-separator + .e-breadcrumb-separator {
        padding-left: 0;
        margin-left: -8px;
    }

    .e-bicons.e-arrow {
        margin-top: -2px;
    }

    .bootstrap .e-bicons.e-arrow,
    .bootstrap4 .e-bicons.e-arrow,
    .bootstrap-dark .e-bicons.e-arrow {
        margin-top: -3px;
    }

    .tailwind .e-bicons.e-arrow,
    .tailwind-dark .e-bicons.e-arrow {
        margin-top: -4px;
    }

    .e-arrow:before {
        content: "\e706";
        font-weight: 800;
        font-size: 9px;
    }
</style>